{
    "name": "webhappens/magic-properties",
    "description": "Add a simple fluid interface for getters, setters, accessors and mutators to any PHP class.",
    "keywords": [
        "webhappens",
        "magic-properties",
        "getters",
        "setters"
    ],
    "homepage": "https://github.com/webhappens/magic-properties",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Leicester",
            "email": "sam@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        },
        {
            "name": "Ben Gurney",
            "email": "ben@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/var-dumper": "^4.3|^5.0",
        "phpunit/phpunit": "7.5|^8.2|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WebHappens\\MagicProperties\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebHappens\\MagicProperties\\Tests\\": "tests"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
